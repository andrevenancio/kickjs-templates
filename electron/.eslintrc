{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "document": true,
      "__VERSION__": true,
      "__NODE_ENV__": true
    },
    "rules": {
      "arrow-body-style": [0],
      "global-require": [0],
      "indent": [2, 4],
      "no-console": [0],
      "no-restricted-syntax": [2, 'DebuggerStatement'],
      "react/jsx-filename-extension": [0],
      "react/jsx-indent-props": [2, 4],
      "react/jsx-indent": [2, 4],
      "react/prefer-stateless-function": [0]
    }
}
